<Window x:Class="Menu_Program.Password"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Password" Height="173" Width="300">
    <Grid>
        <Label Content="Please enter password to continue" HorizontalAlignment="Left" Margin="43,10,0,0" VerticalAlignment="Top"/>
        <PasswordBox x:Name="passwordbox" HorizontalAlignment="Left" Margin="43,41,0,0" VerticalAlignment="Top" Width="192"/>
        <Button x:Name="enterbtn" Content="Enter" HorizontalAlignment="Left" Margin="102,66,0,0" VerticalAlignment="Top" Width="75" Click="enterbtn_Click"/>
        <Button Content="close" HorizontalAlignment="Left" Margin="207,113,0,0" VerticalAlignment="Top" Width="75" Click="Button_Click"/>
        <Label x:Name="test" Content="Label" HorizontalAlignment="Left" Margin="32,95,0,0" VerticalAlignment="Top"/>

    </Grid>
</Window>
